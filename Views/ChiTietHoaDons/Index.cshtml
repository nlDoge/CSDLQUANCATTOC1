@model IEnumerable<QLQUANCATTOC.Models.ChiTietHoaDon>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MaThanhToan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TienSanPham)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TienDichVu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaDichVuNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaSanPhamNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MaThanhToan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TienSanPham)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TienDichVu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaDichVuNavigation.MaDichVu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaSanPhamNavigation.MaSanPham)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaHoaDon">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaHoaDon">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaHoaDon">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
